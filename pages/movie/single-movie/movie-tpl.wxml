<import src='../starts/starts-tpl.wxml' />

<template name="movieTpl">

  <view class='movie-container' catchtap='onMovieTab' data-movie-id="{{movieId}}">
    <image class='movie-image' src='{{coverageUrl}}'></image>
    <text class='movie-title'>{{title}}</text>
    <template is="startsTpl" data="{{starts:starts,score:average}}"></template>
  </view>

</template>